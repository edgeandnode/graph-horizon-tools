{
  "name": "@graphprotocol/graph-horizon-cli",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "description": "CLI tool for debugging and inspecting Graph Horizon protocol data",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "build": "tsup && pnpm copy-package-json",
    "build:ts": "tsup",
    "clean": "rimraf dist/*",
    "check": "tsc -b tsconfig.json",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "copy-package-json": "tsx scripts/copy-package-json.ts"
  },
  "devDependencies": {
    "@effect/cli": "^0.69.0",
    "@effect/platform": "^0.90.3",
    "@effect/platform-node": "^0.96.0",
    "@effect/vitest": "^0.25.1",
    "@types/node": "^22.5.2",
    "tsup": "^8.2.4",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^3.2.0"
  },
  "dependencies": {
    "@effect/schema": "^0.75.5",
    "@graphprotocol/toolshed": "^0.6.16",
    "effect": "^3.17.7",
    "ethers": "6.13.7"
  }
}
